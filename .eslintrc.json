    "eslint:recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:astro/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {},
